{
  "name": "amplify-arc",
  "version": "1.0.0",
  "description": "Premium cross-platform web and mobile application for Amplify ARC agency",
  "main": "index.js",
  "scripts": {
    "start:web": "cd web && npm start",
    "start:mobile": "cd mobile && npm start",
    "start": "node start-dev.js",
