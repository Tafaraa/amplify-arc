    "test:web": "cd web && npm test",
    "test:mobile": "cd mobile && npm test"
  },
  "keywords": [
    "react",
    "react-native",
    "cross-platform",
    "agency",
    "premium"
  ],
  "author": "Amplify ARC",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "child_process": "^1.0.2",
    "path": "^0.12.7",
    "fs": "0.0.1-security"
  }
}
